  <% var ptr = 0; %>
  <% for (var i = 0; i < 3; i++) { %>
     <div class="row">
       <% for (var j = 0; j < 3; j++) { %>
	  <% var mypkg = pkgs[ptr]; ptr++; %>
	  <div class="col-md-4 text-center">
	    <a href="/pkg/<%= mypkg.Package %>"><h2><%= mypkg.Package %></h2></a>
	    <h4><%= mypkg.Title %></h4>
	    <p><%= mypkg.Version %>, published
	      <%= mypkg.timeago %>, by <%= mypkg.Maintainer %></p>
	  </div>
       <% } %>
     </div>
  <% } %>
